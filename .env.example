# FRbox Environment Configuration

# Image processing limits
MAX_IMAGE_SIZE=2097152
MAX_IMAGE_WIDTH=640
MAX_FACES=1

# Embedding settings (face_recognition uses 128-dim by default)
EMBEDDING_DIM=128
SIMILARITY_THRESHOLD=0.95

# Security settings

# API keys - comma-separated list of valid keys (empty = no auth, development mode)
# Generate secure keys using: python -c "import secrets; print(secrets.token_urlsafe(32))"
API_KEYS=

# CORS origins - comma-separated list of allowed origins (empty = allow all, development mode)
# Example: https://example.com,https://app.example.com
ALLOWED_ORIGINS=

# Rate limiting - requests per minute per API key or IP address
RATE_LIMIT_PER_MINUTE=60
